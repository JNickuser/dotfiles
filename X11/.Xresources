URxvt*font: xft:DejaVuSansMono:size=14:antialias=true
URxvt*letterSpace: -1

URxvt*borderLess: false
URxvt*externalBorder: 0
URxvt*internalBorder: 4

URxvt*scrollBar: false

URxvt*saveLines: 5000

URxvt*perl-ext-common: default,tabbedex

URxvt*keysym.Control-n: perl:tabbedex:new_tab
URxvt*keysym.Control-k: perl:tabbedex:next_tab
URxvt*keysym.Control-j: perl:tabbedex:prev_tab

URxvt*keysym.Control-r: perl:tabbedex:rename_tab
URxvt*keysym.Control-h: perl:tabbedex:move_tab_left
URxvt*keysym.Control-l: perl:tabbedex:move_tab_right

URxvt*tabbedex.tarbar-fg: 15
URxvt*tabbeddex.tarbat-bg: 236
URxvt*tabbedex.tab-fg: 110
URxvt*tabbedex.tab-bg: 236

URxvt*tabbedex.transparent: true
URxvt*tabbedex.new-button:false
